using System; 

class TESTE {

        public static void Main()
        {
           
            int N, X, i;

            int Interval = 0;
            int outInterval = 0;

           N = int.Parse(Console.ReadLine());

            for (i = 0; i < N; i++)
            {
                X = int.Parse(Console.ReadLine());

                if (X >=10 && X<=20 ) // Como o intervalo no enunciado estar entre [] isso indica que o intervalo Ã© fecha e incluiu os valores 10 e 20. 
                {
                  Interval ++;
                  
                  
                }
                else                                                        
                {
                  outInterval++;
                 
                  
                }

            }
            Console.WriteLine("{0} in", Interval);
            Console.WriteLine("{0} out", outInterval);


            Console.ReadLine();
        }

}
